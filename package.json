{
    "name": "micro-services",
    "version": "1.0.0",
    "description": "Microservice building in typescript",
    "main": "server.js",
    "scripts": {
        "build": "tsc",
        "start": "tsc  && node \"dist/server.js\" ",
        "test": "ts-mocha test/**/*.test.ts --reporter mochawesome",
        "test:coverage": "nyc -r -e .ts -x \"tests/*.test.ts\" mocha -r ts-node/register test/**/*.test.ts",
        "lint:console": "npm run tslint -- --force \"src/**/*.ts\" ",
        "tslint": "tslint",
        "inspect": "node --inspect --require ts-node/register  \"src/server.ts\" "
    },
    "author": "Deepak panwar",
    "license": "ISC",
    "dependencies": {
        "@hapi/joi": "^15.1.0",
        "aes-cross": "^1.0.9",
        "app-root-path": "^2.1.0",
        "body-parser": "^1.18.2",
        "compression": "^1.7.2",
        "cookie-parser": "^1.4.3",
        "cors": "^2.8.4",
        "dotenv": "^2.0.0",
        "express": "^4.16.3",
        "helmet": "^3.14.0",
        "http-status-codes": "^1.3.0",
        "jsonwebtoken": "^8.5.1",
        "mongo-uri-builder": "^2.0.0",
        "morgan": "^1.9.0",
        "mysql": "^2.17.1",
        "response-time": "^2.3.2",
        "typeorm": "^0.2.20",
        "url": "^0.11.0",
        "uuid": "^3.3.2",
        "winston": "^3.1.0"
    },
    "devDependencies": {
        "@types/compression": "^1.0.1",
        "@types/cookie-parser": "^1.4.2",
        "@types/cors": "^2.8.6",
        "@types/express": "^4.17.2",
        "@types/helmet": "0.0.45",
        "@types/mocha": "^5.2.7",
        "@types/morgan": "^1.7.37",
        "@types/node": "^12.7.2",
        "chai": "^4.2.0",
        "eslint-config-prettier": "^3.3.0",
        "eslint-plugin-prettier": "^3.0.0",
        "mocha": "^5.2.0",
        "mochawesome": "^3.1.2",
        "nodemon": "^1.17.5",
        "prettier": "^1.15.2",
        "sinon": "^7.1.1",
        "ts-mocha": "^2.0.0",
        "ts-node": "^7.0.1",
        "tslint": "^5.9.1",
        "tslint-config-airbnb": "^5.11.1",
        "tslint-config-prettier": "^1.16.0",
        "typescript": "3.5"
    }
